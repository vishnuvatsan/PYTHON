n=int(input("Enter the year"))
a=n-2000
b=a//4
c=a-b
d=b*2
e=c+d
f=e%7
match f:
    case 0:
        print("sunday")
    case 1:
        print("monday")
    case 2:
        print("tuesday")
    case 3:
        print("wednesday")
    case 4:
        print("thursday")
    case 5:
        print("friday")
    case 6:
        print("saturday")
    case _:
        print()
